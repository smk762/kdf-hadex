#!/command/with-contenv bash
# shellcheck shell=bash
set -euo pipefail
echo "[kdf] Fixing permissions for setup scripts..."

# Ensure all init scripts have execute permissions
chmod +x /etc/cont-init.d/* 2>/dev/null || true
chmod +x /etc/services.d/kdf/* 2>/dev/null || true

echo "[kdf] Permissions fixed."
